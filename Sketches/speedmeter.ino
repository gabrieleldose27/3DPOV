volatile float nextPixelMicros = 0;
volatile bool interrupted;
volatile bool running;
elapsedMicros sinceMagnet;

volatile uint32_t currentCharIndex = 0;
volatile uint32_t currentColumn = 0;
volatile char currentChar;
volatile bool finished = false;
String displayedString;

const uint32_t latchPin = 5;
const uint32_t outputEnable = 4;
const uint32_t hallpin = 20;

SPISettings mySettings(48000000, LSBFIRST, SPI_MODE0);

void setup(void)
{
	pinMode(hallpin, INPUT_PULLUP);
	pinMode(outputEnable, OUTPUT);
	pinMode(latchPin, OUTPUT);
	digitalWrite(outputEnable, LOW);

	SPI.begin();
	
	attachInterrupt(hallpin, timerUpdate, FALLING);
}

void loop(void) 
{
	if (running && sinceMagnet > nextPixelMicros)
	{
		sendData();
	}
}

void timerUpdate(void)
{
	interrupted = true;
	running = true;

	periodDuration = sinceMagnet;
	sinceMagnet = 0;

	// max() to ensure that longer strings are not cut off, but squeezed together
	microsPerPixel = ((float) periodDuration) / max((uint32_t) 128, 6 + 6*displayedString.length());

	currentPixel = 0;
	nextPixelMicros = 0;

	beginNewRotation();
} 

void sendData(void)
{
	interrupted = false;

	// The following block displays a character (or a blank column between them)
	if(!finished)
	{
		SPI.beginTransaction(mySettings);

		// Each character is 5 px wide + 1 px of keming, so 6 px
		currentColumn = currentPixel % 6;
		
		// If we're in the 'kerning' column...
		if(currentColumn == 5)
		{
			// Display an empty column and go to the next character
			sendEmptyBytes(60);
			pulseLatch();
			moveToNextChar();
		}
		else
		{
			// Fill the upper 9 boards with 0's
			sendEmptyBytes(54);

			// Send a column of a character to the lowest PCB
			for(int i = 5; i >= 0; i--)
			{
				// currentChar - 32 because the ascii array starts at 32
				SPI.transfer(ascii[currentChar - 32][currentColumn][i]);
			}
			pulseLatch();
		}

		SPI.endTransaction();
	}
	if (!interrupted)
	{
		currentPixel++;
		nextPixelMicros += microsPerPixel;
	}
}

// Sends specified number of empty bytes
inline void sendEmptyBytes(int count)
{
	for(int i=0; i<count; i++)
	{
		SPI.transfer(0x00);
	}
}

// Latches the sent data to the outputs
inline void pulseLatch(void)
{
	digitalWrite(latchPin, HIGH);
	delayMicroseconds(1);
	digitalWrite(latchPin, LOW);
}

// Moves to the next character and checks if string is finished
void moveToNextChar(void)
{
	currentCharIndex++;
	if(currentCharIndex > displayedString.length())
	{
		finished = true;
	}
	else
	{
		currentChar = displayedString.charAt(displayedString.length() - currentCharIndex);
	}
}

void beginNewRotation(void)
{
	// Writing '|' in here will display the 'Âµ' character (ASCII index 124)
	displayedString = String(1000000.0/periodDuration) + "Hz "+ String(periodDuration) + "|s " + String(1538123.8/periodDuration) + "km/h ";
	currentCharIndex = 0;
	moveToNextChar(); // currentCharIndex becomes 1 and currentChar is given the proper value
	finished = false;
}

